{
    "name": "perevorotcom/laraveloctober",
    "description": "",
    "keywords": [
        "framework",
        "laravel",
        "octobercms"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Andrey Lanko",
        "email": "lanko@perevorot.com"
    }],
    "type": "project",
    "require": {
	"php": "^7.2",
